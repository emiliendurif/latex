\newcommandx{\UPSTIecartsGrosseFleche}[3][2=0,3=0]{
  \begin{scope}[xshift=#2 cm,yshift=#3 cm]
    \draw[ecartsGrosseFleche,scale=0.3] (-7,0) -- (-9,1) -- (8,1) -- (8,1.8) -- (12,0) -- (8,-1.8) -- (8,-1) -- (-9,-1) -- (-7,0);
    \draw(0.3,-0.05) node{#1};
  \end{scope}
}

% #1: échelle, #2-4: mise en valeur des écarts étudiés, #5-7: chemin des images si besoin de les changer
\newcommandx {\UPSTIdiagrammeEcarts}[7][1=,2=0,3=0,4=0,5=,6=,7=]{
  \begin{tikzpicture}[scale=\ifstrempty{#1}{0.9}{#1}, every node/.style={scale=\ifstrempty{#1}{0.9}{#1}}]
    \tikzset{ecartsGrosseFleche/.style={fill=ecartsCouleurFondGrosseFleche,text=ecartsCouleurTexteGrosseFleche},ecartsDoubleFleche/.style={<->,>=latex,very thick}}
    \UPSTIecartsGrosseFleche{Système souhaité}[0][1.8]
    \UPSTIecartsGrosseFleche{Système réel}
    \UPSTIecartsGrosseFleche{Système simulé}[0][-1.8]
    
    \node[align=center] at (5,1.8) {Performances\\attendues};
    \node[align=center] at (5,0) {Performances\\mesurées};
    \node[align=center] at (5,-1.8) {Performances\\simulées};
    
    \draw [ecartsDoubleFleche] (6.5,0.1) -- (6.5,1.9);
    \node[rotate=90] at (6.9,1) {Écart 1};
    \draw [ecartsDoubleFleche] (6.5,-0.1) -- (6.5,-1.9);
    \node[rotate=90] at (6.9,-1) {Écart 2};
    \draw [ecartsDoubleFleche] (7.5,1.9) -- (7.5,-1.9);
    \node[rotate=90] at (7.9,0) {Écart 3};
    
    % Intégration avec UPSTI_Document
    % (si on utilise ce package avec UPSTI_Document, UPSTIcheminImages est prioritaire par rapport à UPSTIcheminDossierImagesDiagrammeEcarts)
    \ifdef{\UPSTIcheminImages}{\renewcommand{\UPSTIcheminDossierImagesDiagrammeEcarts}{\UPSTIcheminImages}}{}
    
    \ifthenelse{\equal{#5}{}}
      {\node at (-4.2,1.8) {\includegraphics[width=2.7cm,height=1.7cm,keepaspectratio]{\UPSTIcheminDossierImagesDiagrammeEcarts ecartsPhoto1.png}};}
      {\node at (-4.2,1.8) {\includegraphics[width=2.7cm,height=1.7cm,keepaspectratio]{#5}};}
    \ifthenelse{\equal{#6}{}}
      {\node at (-4.2,0) {\includegraphics[width=2.7cm,height=1.7cm,keepaspectratio]{\UPSTIcheminDossierImagesDiagrammeEcarts ecartsPhoto2.png}};}
      {\node at (-4.2,0) {\includegraphics[width=2.7cm,height=1.7cm,keepaspectratio]{#6}};}
    \ifthenelse{\equal{#7}{}}
      {\node at (-4.2,-1.8) {\includegraphics[width=2.7cm,height=1.7cm,keepaspectratio]{\UPSTIcheminDossierImagesDiagrammeEcarts ecartsPhoto3.png}};}
      {\node at (-4.2,-1.8) {\includegraphics[width=2.7cm,height=1.7cm,keepaspectratio]{#7}};}
    
    % Mise en valeur des écarts étudiés
    \ifnumequal{1}{#2}{\draw[dashed,very thick,ecartsCouleurEcart1] (6.35,0.05) rectangle (6.65,1.95);}{}
    \ifnumequal{1}{#3}{\draw[dashed,very thick,ecartsCouleurEcart2] (6.35,-0.05) rectangle (6.65,-1.95);}{}
    \ifnumequal{1}{#4}{\draw[dashed,very thick,ecartsCouleurEcart3] (7.35,1.95) rectangle (7.65,-1.95);}{}
    
  \end{tikzpicture}
}


