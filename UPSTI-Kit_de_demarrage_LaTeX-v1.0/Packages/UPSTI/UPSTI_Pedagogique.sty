%--------------------------------------------------------%
% Liste des compétences
%--------------------------------------------------------%
% Filière (1er argument):
% 1- TSI
% 2- PTSI/PT
% 3- PCSI/PSI  
% 4- MPSI/MP 
% 5- ATS 
\newcommandx{\UPSTIcompetence}[3][1=,3=]{

  % Initialisation
  \ifdef{\UPSTIcouleurCompetence}{}{\newcommand{\UPSTIcouleurCompetence}{couleurCompetenceA}}
  \ifdef{\UPSTIintituleCompetence}{}{\newcommand{\UPSTIintituleCompetence}{Compétence}}
  \ifdef{\UPSTIcompCouleur}{\renewcommand{\UPSTIcompCouleur}{}}{\newcommand{\UPSTIcompCouleur}{}}
  \ifdef{\UPSTIcompIntitule}{\renewcommand{\UPSTIcompIntitule}{}}{\newcommand{\UPSTIcompIntitule}{}}
  
  % Intégration avec EPBCours
  \UPSTIdetermineIdFiliere

  % Filiere (par défaut PTSI/PT - TSI - ATS) - le 1er paramètre facultatif supplante idFiliere
  \ifstrempty{#1}
  {
    \ifdef{\UPSTIidFiliereTmp}{\renewcommand{\UPSTIidFiliereTmp}{\UPSTIidFiliere}}{\newcommand{\UPSTIidFiliereTmp}{\UPSTIidFiliere}}
  }{
    \ifdef{\UPSTIidFiliereTmp}{\renewcommand{\UPSTIidFiliereTmp}{#1}}{\newcommand{\UPSTIidFiliereTmp}{#1}}
  }

  % On définit les compétences suivant la filière 
  \ifcase\UPSTIidFiliereTmp
    Erreur dans le code filière (=0)

    %--------------------------------------------------------%
    % PTSI/PT
    %--------------------------------------------------------%
    \or
    \ifthenelse{\equal{#2}{A}}{\renewcommand{\UPSTIcompCouleur}{couleurCompetenceA}\renewcommand{\UPSTIcompIntitule}{Analyser}}{}
