%--------------------------------------------------------%
% Diagramme des compétences
%--------------------------------------------------------%
% Personnalisation
\newcommand*{\UPSTIcompetencesRayon}{10em}
\newcommand*{\UPSTIcompetencesIncrement}{25}
\newcommand*{\UPSTIcompetenceActifInactif}[1]{\ifnumequal{0}{#1}{inactif}{actif}} % Gestion des actifs/inactifs

% #1: echelle, #2: filiere*, #3-9: activation ou non des compétences (activées par défaut)
% 1- PTSI/PT
% 2- PCSI/PSI (PC ?)  
% 3- MPSI/MP 
% 4- TSI
% 5- ATS 
\newcommandx{\UPSTIdiagrammeCompetences}[9][1=,2=,3=1,4=1,5=1,6=1,7=1,8=1,9=1]	%
{
  % Intégration avec UPSTI_Document
  \UPSTIdetermineIdFiliere

  % Filiere: le 2ème paramètre facultatif écrase idFiliere
  \ifstrempty{#2}
  {
    \ifdef{\UPSTIidFiliereTmp}{\renewcommand{\UPSTIidFiliereTmp}{\UPSTIidFiliere}}{\newcommand{\UPSTIidFiliereTmp}{\UPSTIidFiliere}}
  }{
    \ifdef{\UPSTIidFiliereTmp}{\renewcommand{\UPSTIidFiliereTmp}{#2}}{\newcommand{\UPSTIidFiliereTmp}{#2}}
  }

  % Un switch pour "fusionner" les filières qui ont le même diagramme de compétence: (ex: PTSI/PT-TSI-ATS) 
  \ifcase\UPSTIidFiliereTmp
    Erreur dans UPSTIidFiliere (=0)
    \or %PTSI/PT
      \ifdef{\UPSTIidFilierePourDiagrammeCompetences}{\renewcommand{\UPSTIidFilierePourDiagrammeCompetences}{0}}{\newcommand{\UPSTIidFilierePourDiagrammeCompetences}{0}}
    \or %PCSI/PSI 
      \ifdef{\UPSTIidFilierePourDiagrammeCompetences}{\renewcommand{\UPSTIidFilierePourDiagrammeCompetences}{1}}{\newcommand{\UPSTIidFilierePourDiagrammeCompetences}{1}}
    \or %MPSI/MP 
      \ifdef{\UPSTIidFilierePourDiagrammeCompetences}{\renewcommand{\UPSTIidFilierePourDiagrammeCompetences}{2}}{\newcommand{\UPSTIidFilierePourDiagrammeCompetences}{2}}
    \or %TSI 
      \ifdef{\UPSTIidFilierePourDiagrammeCompetences}{\renewcommand{\UPSTIidFilierePourDiagrammeCompetences}{0}}{\newcommand{\UPSTIidFilierePourDiagrammeCompetences}{0}}
    \or %ATS 
      \ifdef{\UPSTIidFilierePourDiagrammeCompetences}{\renewcommand{\UPSTIidFilierePourDiagrammeCompetences}{0}}{\newcommand{\UPSTIidFilierePourDiagrammeCompetences}{0}}
    \else Erreur dans UPSTIidFiliereTmp (Trop grand)
  \fi

  % Dessin du diagramme  
  \begin{tikzpicture}[scale=\ifstrempty{#1}{0.9}{#1}, every node/.style={scale=\ifstrempty{#1}{0.9}{#1}}]
    
    % Styles des boîtes actives/inactives
    \tikzset{
      actif/.style={rounded corners=0.4em,draw,fill=competencesCouleurFondFeuilleSelected,text=competencesCouleurTexteFeuilleSelected,minimum height=2em},
      inactif/.style={rounded corners=0.4em,draw,dashed,fill=competencesCouleurFondFeuille,text=competencesCouleurTexteFeuille,minimum height=2em}
    }
    
    \ifcase\UPSTIidFilierePourDiagrammeCompetences
      % 0: PTSI, PT, TSI, ATS
      \node[\UPSTIcompetenceActifInactif{#3}] (A) at (180-\UPSTIcompetencesIncrement/2-\UPSTIcompetencesIncrement:\UPSTIcompetencesRayon) {Communiquer};
      \node[\UPSTIcompetenceActifInactif{#4}] (B) at (180-\UPSTIcompetencesIncrement/2:\UPSTIcompetencesRayon) {Réaliser};
      \node[\UPSTIcompetenceActifInactif{#5}] (C) at (180+\UPSTIcompetencesIncrement/2:\UPSTIcompetencesRayon) {Expérimenter};
      \node[\UPSTIcompetenceActifInactif{#6}] (D) at (180+\UPSTIcompetencesIncrement/2+\UPSTIcompetencesIncrement:\UPSTIcompetencesRayon) {Concevoir};
      \node[\UPSTIcompetenceActifInactif{#7}] (E) at (\UPSTIcompetencesIncrement:\UPSTIcompetencesRayon) {Analyser};
      \node[\UPSTIcompetenceActifInactif{#8}] (F) at (0:\UPSTIcompetencesRayon) {Modéliser};
      \node[\UPSTIcompetenceActifInactif{#9}] (G) at (-\UPSTIcompetencesIncrement:\UPSTIcompetencesRayon) {Résoudre};
      \draw (C) .. controls +(9em,0) and (0,0) .. (0,0);
      \draw (D) .. controls +(9em,0) and (0,0) .. (0,0);
      \draw (E) .. controls +(-9em,0) and (0,0) .. (0,0);
      \draw (F) .. controls +(-9em,0) and (0,0) .. (0,0);
      \draw (G) .. controls +(-9em,0) and (0,0) .. (0,0);
    
    \or 
      % 1: PCSI, PSI
      \node[\UPSTIcompetenceActifInactif{#3}] (A) at (180-\UPSTIcompetencesIncrement:\UPSTIcompetencesRayon) {Communiquer};
      \node[\UPSTIcompetenceActifInactif{#4}] (B) at (180:\UPSTIcompetencesRayon) {Concevoir};
      \node[\UPSTIcompetenceActifInactif{#5}] (C) at (180+\UPSTIcompetencesIncrement:\UPSTIcompetencesRayon) {Expérimenter};
      \node[\UPSTIcompetenceActifInactif{#6}] (D) at (\UPSTIcompetencesIncrement:\UPSTIcompetencesRayon) {Analyser};
      \node[\UPSTIcompetenceActifInactif{#7}] (E) at (0:\UPSTIcompetencesRayon) {Modéliser};
      \node[\UPSTIcompetenceActifInactif{#8}] (F) at (-\UPSTIcompetencesIncrement:\UPSTIcompetencesRayon) {Résoudre};
      \draw (C) .. controls +(9em,0) and (0,0) .. (0,0);
      \draw (D) .. controls +(-9em,0) and (0,0) .. (0,0);
      \draw (E) .. controls +(-9em,0) and (0,0) .. (0,0);
      \draw (F) .. controls +(-9em,0) and (0,0) .. (0,0);
    
    \else 
      % 2: MPSI, MP
      \node[\UPSTIcompetenceActifInactif{#3}] (A) at (180-\UPSTIcompetencesIncrement/2:\UPSTIcompetencesRayon) {Communiquer};
      \node[\UPSTIcompetenceActifInactif{#4}] (B) at (180+\UPSTIcompetencesIncrement/2:\UPSTIcompetencesRayon) {Expérimenter};
      \node[\UPSTIcompetenceActifInactif{#5}] (C) at (\UPSTIcompetencesIncrement:\UPSTIcompetencesRayon) {Analyser};
      \node[\UPSTIcompetenceActifInactif{#6}] (D) at (0:\UPSTIcompetencesRayon) {Modéliser};
      \node[\UPSTIcompetenceActifInactif{#7}] (E) at (-\UPSTIcompetencesIncrement:\UPSTIcompetencesRayon) {Résoudre};
      \draw (C) .. controls +(-9em,0) and (0,0) .. (0,0);
      \draw (D) .. controls +(-9em,0) and (0,0) .. (0,0);
      \draw (E) .. controls +(-9em,0) and (0,0) .. (0,0);
      \fi
      \draw (A) .. controls +(9em,0) and (0,0) .. (0,0);
      \draw (B) .. controls +(9em,0) and (0,0) .. (0,0);
    
    % Bloc central
    \node[rounded corners=0.4em,draw,anchor=center,thick,fill=competencesCouleurFondCentrale,minimum height=3em] (centre) at (0,0) {\textbf{\color{competencesCouleurTexteCentrale}Compétences}};

    % Flèche rouge
    \begin{scope} [rotate=65]
    \draw[>=latex,->,color=competencesCouleurFleche,very thick] (\UPSTIcompetencesRayon/2,0) arc (0:182:\UPSTIcompetencesRayon/2);
    \end{scope}
    \begin{scope} [rotate=245]
    \draw[>=latex,->,color=competencesCouleurFleche,very thick] (\UPSTIcompetencesRayon/2,0) arc (0:182:\UPSTIcompetencesRayon/2);
    \end{scope}
  
  \end{tikzpicture}
}

